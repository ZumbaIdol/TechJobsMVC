<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

        
    @if (ViewBag.jobs.Count > 0)
    {
        @foreach (Dictionary<string, string> item in ViewBag.jobs)
        {<table>
   
        @foreach (KeyValuePair<string, string> pair in item)
        {

            <tr>
                <th>@pair.Key</th>
                <td>@pair.Value</td>
                
            </tr>




        }
    </table>
        }

    }
}